<div id="depositContainer" class="container">
	<div class="row">
		<div class="col s7">
			<div id="bannertitle" class="card-panel grey darken-2 valign-wrapper">
				<img src="/imgs/key.png" id="key" class="valign"/><span class="white-text">&nbsp;&nbsp;&nbsp;Inventory</span>
			</div>
			<div class="input-field white-text">
				<input placeholder="Search Inventory" id="seach-inv" type="text" class="validate">
			</div>
		</div>

		<div class="col s5">
			<div id="bannertitle" class="card-panel grey darken-1 valign-wrapper">
				<img src="/imgs/key.png" id="key" class="valign"/><span class="white-text">&nbsp;&nbsp;&nbsp;Deposit</span>
			</div>
			<div id="bannertitle" class="card-panel green white-text">
				<div class="center">
					<span class="white-text" style="font-weight: 600 !important;">Deposit Now</span>
					<br />
					<span >250,000 Credits</span>
				</div>
			</div>
		</div>
	</div>

	<div class="row white-text" id="depositWrapper">
		{{>inventoryitems}}
		{{>itemsfordeposit}}
	</div>

	<div class="row" id="depositFooter">
		<div class="col s12">
			<div id="bannertitle" class="card-panel grey darken-3">
				<img class="right" src="/imgs/csgoanalyst.png" id="csgoanalyst"/><span class="right white-text">Prices brought to you by:&nbsp;&nbsp;</span><span class="white-text">We do not accpet stickers or items under 60<i class="material-icons tinyer">grade</i></span>
			</div>
		</div>
	</div>
</div>
